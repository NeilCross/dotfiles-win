{
    "window.title": "${rootName}${separator}${appName}",
    "glsl-canvas.useCompactFormatter": false,
    "terminal.integrated.fontFamily": "'Consolas NF','Cascadia Mono PL', Consolas, 'Courier New', monospace",
    "git.confirmSync": false,
    "[markdown]": {
        "editor.rulers": [
            79,
            120
        ]
    },
    "spellright.language": [
        "en-GB"
    ],
    "editor.unicodeHighlight.nonBasicASCII": true,
    "git.ignoreLegacyWarning": true,
    "window.autoDetectColorScheme": true,
    "editor.inlineSuggest.enabled": true,
    "github.copilot.enable": {
        "*": true,
        "yaml": false,
        "plaintext": false,
        "markdown": false
    },
    "editor.renderControlCharacters": false,
    "vscode-kubernetes.log-viewer.follow": false,
    "vscode-kubernetes.log-viewer.timestamp": false,
    "vscode-kubernetes.log-viewer.wrap": false,
    "window.zoomLevel": 1
}